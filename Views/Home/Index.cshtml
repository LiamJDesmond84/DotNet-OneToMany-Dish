@model List<Dish>

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="CreateChefView">Add Chef</a>
    <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="CreateDishView">Add Dish</a>


    @if(Model != null){


        foreach(var x in Model)
        {
            <p>Chef: @x.Chef.Name</p>
            <p>Description: @x.Description</p>

        }






    }

        </div>
